{
  "name": "__MSG_name__",
  "version": "1.0.0",
  "manifest_version": 3,
  "default_locale": "en",
  "description": "__MSG_description__",
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "run_at": "document_end",
      "js": ["popup.js"]
    }
  ],
  "permissions": [
    "tabs", "activeTab", "bookmarks", "browsingData", "history", "scripting", "search"
  ]
}
